## enable mod_rewrite
RewriteEngine on
## deny access to *.py, *.pyhtml, and *.cache
RewriteRule \.(py|pyhtml|cache)$ - [R=404,L]
## rewrite only if requested file is not found
RewriteCond %{SCRIPT_FILENAME} !-f
## handle request to *.html by pytenjin.cgi
RewriteRule \.html$ pytenjin.cgi
## handle request to root or other directory by pytenjin.cgi
RewriteRule ^$ pytenjin.cgi
RewriteRule /$ pytenjin.cgi
